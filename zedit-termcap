# Sample termcap entry for Zedit.
xterm|linux:\
	:is=\E[1;25r\E[25;1H:\
	:cm=\E[%i%d;%dH:\
	:ce=\E[K:\
	:cl=\E[H\E[2J:\
	:me=\E[m:\
	:so=\E[7m:\
	:md=\E[1m:\
	:ku=\E[A:\
	:kd=\E[B:\
	:kr=\E[C:\
	:kl=\E[D:\
	:kI=\E[2~:\
	:kD=\E[3~:\
	:kP=\E[5~:\
	:kN=\E[6~:\
	:kh=\E[7~:\
	:@7=\E[8~:\
	:k1=\E[11~:\
	:k2=\E[12~:\
	:k3=\E[13~:\
	:k4=\E[14~:\
	:k5=\E[15~:\
	:k6=\E[17~:\
	:k7=\E[18~:\
	:k8=\E[19~:\
	:k9=\E[20~:\
	:k;=\E[21~:\
	:F1=\E[23~:\
	:F2=\E[24~:
